{
  "name": "@unraidclaw/server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup src/index.ts --format esm --target node22 --dts --clean --external @unraidclaw/shared",
    "bundle": "tsup src/index.ts --format esm --target node22 --clean --no-splitting --noExternal @unraidclaw/shared",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@unraidclaw/shared": "workspace:*",
    "fastify": "^5.2.0"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "@types/node": "^22.0.0"
  }
}
